<section class="form">
  <%= form_with model: @user, url: "/register", local:true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :address %>
    <%= form.text_field :address %>
    <%= form.label :city %>
    <%= form.text_field :city %>
    <%= form.label :state %>
    <%= form.text_field :state %>
    <%= form.label :zip %>
    <%= form.text_field :zip %>
    <%= form.label :email %>
    <%= form.email_field :email, value: nil %>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= form.label :password_confirmation %>
    <%= form.password_field :password_confirmation %>
    <%= form.submit "Submit" %>
  <% end %>
</section>
